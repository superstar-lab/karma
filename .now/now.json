{
  "version": 2,
  "scope": "fotontech",
  "builds": [
    {
      "src": "packages/web/package.json",
      "use": "@now/next@canary"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "packages/web/$1"
    }
  ],
  "build": {
    "env": {
      "NODE_ENV": "production",
      "RELEASE_STAGE": "staging"
    }
  }
}