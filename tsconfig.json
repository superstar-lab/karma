{
  "compilerOptions": {
    "target": "esnext", 
    "module": "esnext", 
    "moduleResolution": "node",
    "lib": [ 
      "esnext",
      "dom",
      "dom.iterable"
    ],
    "allowJs": true,                       
    "forceConsistentCasingInFileNames": true,
    "jsx": "preserve",
    "outDir": "./distTs", 
    "noEmit": true, 
    "isolatedModules": true,
    "strict": false, 
    "noImplicitAny": false, 
    "noUnusedLocals": true, 
    "noUnusedParameters": false, 
    "noImplicitReturns": true, 
    "baseUrl": "./", 
    "paths": {
      "*": [
        "@types/*"
      ]
    }, 
    "resolveJsonModule": true,
    "esModuleInterop": true, 
    "skipLibCheck": true
  },
  "exclude": [
    "node_modules",
    "build"
  ]
}